<tl-data-table [dataTableObj]="dataTableObj" [dataTableValue]="dataTableValue" [pagingBoxObj]="pagingBoxObj" (onPageChange)="pageChange($event)">
  <template tl-template="serial_number" let-rowData>
    <p>{{rowData._index+1}}</p>
  </template>
  <template tl-template="account_title" let-rowData>
    <!--<p class="update-time margin-bottom8">最后修改时间&nbsp;:&nbsp;{{rowData.update_time}}</p>-->
    <p class="update-time margin-bottom8">最后修改时间&nbsp;:&nbsp;{{rowData.update_time|date:'yyyy-MM-dd HH:mm:ss'}}</p>
    <p>{{rowData.account_title}}</p>
  </template>
  <template tl-template="account_id" let-rowData>
    <p>{{rowData.account_id}}</p>
  </template>
  <template tl-template="account_role" let-rowData>
    <p>{{getAccountRole(rowData.account_role)}}</p>
  </template>
  <template tl-template="oper" let-rowData>
    <tl-operating-button [title]="'编辑'" [className]="'opraEdit'" (onClick)="edit($event,rowData)"></tl-operating-button>
    <tl-operating-button [title]="'删除'" [className]="'opraDelete'" (onClick)="delete($event,rowData)"></tl-operating-button>
  </template>
</tl-data-table>
