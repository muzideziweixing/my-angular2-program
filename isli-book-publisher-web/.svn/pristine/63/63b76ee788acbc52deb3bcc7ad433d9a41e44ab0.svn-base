<div [class.prompts-box-position]="promptsBoxObj.positionFlag" *ngIf="ifPromptsBox" [style.display]="promptsBoxObj.visible?'flex':'none'"
     [@dialogState]="promptsBoxObj._showAnimations ? 'visible' : 'hidden'">
  <div class="prompts-box" [ngClass]="{'prompts-box-error':promptsBoxObj.error}" [ngStyle]="promptsBoxStyles">
    <span class="prompts-box-name">{{promptsBoxObj?.name}}</span>
  </div>
</div>
