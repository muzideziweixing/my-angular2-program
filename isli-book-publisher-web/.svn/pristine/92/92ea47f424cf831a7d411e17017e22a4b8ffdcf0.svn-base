/*分页*/
.tl-paginator{ height: 23px; display: flex; justify-content: flex-end; padding-top: 23px; padding-bottom: 23px;}
.ui-paginator{ height: 23px; }
.ui-paginator-pages{ padding:0; display: inline-block; vertical-align: top; }
.ui-paginator-page{ width: 21px; height: 21px; line-height: 21px; color: #737373; text-align: center; margin-left: 5px; border: 1px solid #bababa; display: inline-block;}
.ui-state-active{ background: #79c3cc; color: #fff; border: 1px solid #79c3cc;}
.ui-paginator-prev,.ui-paginator-next{ width: 23px; height: 23px; border: 0; display: inline-block;}
.ui-paginator-prev{ background:url(assets/images/pre.png) no-repeat ;}
.ui-paginator-next{ background:url(assets/images/next.png) no-repeat ;}
.ui-paginator-next:hover,.ui-paginator-next:active,.ui-paginator-prev:hover,.ui-paginator-prev:active{ background-position: 0 -23px;}
.ui-paginator-next.ui-state-disabled,.ui-paginator-next.ui-state-disabled:active,.ui-paginator-next.ui-state-disabled:hover,
.ui-paginator-prev.ui-state-disabled,.ui-paginator-prev.ui-state-disabled:active,.ui-paginator-prev.ui-state-disabled:hover{background-position: 0 -46px;}
.tl-paginator-ext { height:23px; line-height:23px; padding-right: 23px; padding-left: 12px;}
.tl-paginator-ext input{ width: 26px; height: 21px; border: 1px solid #bababa; color: #7e7e7e; text-align: center; vertical-align: top;}
.tl-paginator-ext span{ color: #989898; height:23px; line-height: 23px; display: inline-block;}
.tl-paginator-ext span.page-skip{ display: inline-block; position: static; width: 40px; text-align: center;border: 1px solid #5acedb; color: #5acedb; height: 21px; line-height: 21px; }
.tl-paginator-ext span.page-skip:hover,.tl-paginator-ext span.page-skip:active{ background: #f1fdfe;}
/*日历组件*/
.ui-datepicker{ width:214px;padding:0;border:1px solid silver; position: absolute; z-index: 99; top: 27px;}
.ui-inputtext{ width: 193px; height: 24px; padding: 0 10px; border: 1px solid #a5a5a5; color: #7e7e7e; background:url(assets/images/datePickerDefault.png) no-repeat 187px 1px;}
.ui-inputtext:hover{background:url(assets/images/datePicker.png) no-repeat 187px 1px;}
.ui-datepicker-title { margin: 0 2.3em; height: 28px; line-height: 28px; text-align: center; }
.ui-datepicker-header{ width: 214px;  height: 28px; line-height: 28px; color: #fff; padding: 0; position: relative; background:url(assets/images/date_header_bg.png) top center repeat-x; border-bottom: 1px solid #c7d7ea;}
.ui-datepicker-prev,.ui-datepicker-next{ position: absolute; top: 1.25em; display: inline-block;}
.ui-datepicker-prev { margin-left:5px;background:url(assets/images/dateLeftBg.png) center no-repeat; width:20px; height:28px; font-size:0; left: .125em;}
.ui-datepicker-next { margin-right:5px;background:url(assets/images/dateRightBg.png) center no-repeat; width:20px; height:28px; font-size:0; right: .125em;}
.ui-datepicker-calendar { margin:0; width:214px; background: #fff;}
.ui-datepicker-calendar thead tr { color:#589BA3;border-top:1px solid #c8d2d4;border-bottom:1px solid #c8d2d4;background:#E5F3F5;}
.ui-datepicker-calendar thead tr th span {font-weight:400;color:#589BA3;}
.ui-datepicker-calendar thead tr { text-align:center;font-weight:400;font-size:12px;height:19px;background:#e5f3f5;color:#589ba3;border:0;padding:0;}
.ui-datepicker-calendar tbody tr td { border:0; border-top:1px solid #e7e6e6;border-left:1px solid #e7e6e6;height:22px;line-height:22px;text-align:center;padding:0;}
.ui-datepicker-calendar tbody tr td:nth-of-type(1) {border-left:none;}
.ui-datepicker-calendar tbody tr td a { padding:0; font-size:12px;  width: 28px; height: 21px; display: inline-block; color: #5e5e5e;}
.ui-datepicker-calendar tbody tr td a:hover {background:#f1fdfe;}
.ui-datepicker-calendar tbody tr td a.ui-state-active  { background:#fff; color: #5e5e5e; border: 1px solid #6fccd7;}
.ui-datepicker-calendar tbody tr td a.ui-state-highlight  { background:#6fccd7; color:#fff; border: 1px solid #6fccd7;}
.ui-datepicker-calendar tbody tr td a.ui-state-disabled{ opacity: .35;}
.ui-datepicker-calendar tbody tr td.ui-state-disabled a{ color: #7e7e7e; opacity: .35;}
.ui-datepicker-calendar tbody tr td.ui-state-disabled a:hover{ background: #fff;}
.ui-datepicker-year,.ui-datepicker-month {height:28px;line-height:28px;font-size:13px;font-weight:600;}
.ui-datepicker-year {float:left;text-align:right;width:49%;}
.ui-datepicker-year::after {content:'';}
.ui-datepicker-month::before {content:' - ';}
.ui-datepicker-month {float:right;text-align:left;width:49%;}
.ui-datepicker select{ outline: none; border: 0;}
.ui-datepicker select.ui-datepicker-month,div.ui-datepicker select.ui-datepicker-year {height:28px;line-height:28px;position:relative;top:0;text-align:center;color:#fff;background:transparent;-webkit-appearance:none;background:url(assets/images/down.png) right center no-repeat;margin-top:0;}
.ui-datepicker select.ui-datepicker-month {text-indent:2em;}
.ui-datepicker select.ui-datepicker-year {text-indent:1em;}
.ui-datepicker select.ui-datepicker-month option,div.ui-datepicker select.ui-datepicker-year option {color:#5e5e5e;}
/*语种*/
.ui-dropdown{ width: 203px; height: 24px; line-height: 24px; color: #7e7e7e; padding-left: 10px; background: url(assets/images/drop-down-box/default.png) 192px center no-repeat; border: 1px solid #a5a5a5;}
.ui-dropdown .ui-inputtext{ border: 0; padding: 0;}
.ui-helper-hidden-accessible{ display: none; }
.ui-dropdown-items-wrapper{ max-height: 200px; }
