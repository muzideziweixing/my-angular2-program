@charset "utf-8";
*{margin:0;padding:0;}
html, body, div, span, applet, object, iframe, table, caption, tbody, tfoot, thead, tr, th, td, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, dl, dt, dd, ol, ul, li, fieldset, form, label, legend,input,textarea,button{font-family:"Microsoft YaHei","WenQuanYi Micro Hei",SimHei,tahoma,sans-serif; font-size: 13px; -webkit-touch-callout: none; -webkit-user-select: none; -khtml-user-select: none; -moz-user-select: none; -ms-user-select: none; user-select: none; }
body{font-size:13px;-webkit-text-size-adjust:none; }
table{border-collapse:collapse;border-spacing:0;table-layout:fixed;}
ul li,ol li{list-style-type:none;}
a,button{outline:none; }
a{text-decoration:none;}
img{border:none;vertical-align:top}
input,select,textarea,button {vertical-align:middle;}
button {border:0 none;background-color:transparent;cursor:pointer}
table {width:100%;border-collapse:collapse;}
em{font-style: normal;color: #f40;}
dl,dt,dd{ float: left;}

input,textarea{ padding:0 5px; border: 1px solid #C7C7C7; font-size: 13px; color:#7e7e7e; }
input{ height:24px;line-height: 24px; }
label{ color:#575757; }
span{ color:#888; }

.clearfix:before, .clearfix:after { content:""; display:table; }
.clearfix:after { clear:both; overflow:hidden;}
.clearfix { zoom:1;}

.fl{ float: left;}
.fr{ float: right;}
/*滚动条*/
::-webkit-scrollbar{ width: 8px; background-color: #f2f2f2; }
::-webkit-scrollbar-thumb{ background-color: rgb(213, 213, 213); border-color: rgb(242, 242, 242);}
/*按钮*/
.btn{ padding: 0 30px; height: 32px; line-height: 30px; text-align: center; border-radius: 3px; color: #fff; border: 0; margin-left: 5px; display: inline-block; cursor: pointer;}
.btn-red{ background: #d9624f; border-bottom: 2px solid #ad7066;}
.btn-red:hover{ background: #f06d58; border-bottom: 2px solid #bf7a6f;}
.btn-red:active{ background: #c54b38; border-bottom: 2px solid #9f675e;}
.btn-blue{ background: #2fadbc; border-bottom: 2px solid #257e88;}
.btn-blue:hover{ background: #38c6d7; border-bottom: 2px solid #5ba1aa;}
.btn-blue:active{ background: #25a3b2; border-bottom: 2px solid #538389;}
.btn-gray{ background: #b9b9b9; border-bottom: 2px solid #aaa; cursor: no-drop;}
.btn-select{ width: 73px; height: 32px; border: 1px solid #5acedb; background: #f1fdfe; color: #5acedb; display: inline-block; text-align: center; line-height: 32px; cursor: pointer; margin-left: 10px; margin-top: -3px;}
.btn-select:hover{ background: #f7feff;}
.btn-select:active,.btn-select-active,.btn-select-active:hover,.btn-select-active:active{ background: #e0f9fb;}
.btn-select-disable,.btn-select-disable:hover,.btn-select-disable:active{ width: 73px; height: 32px; background: #f8fdfd; border: 1px solid #96dfe8; color: #8ebbc0; display: inline-block; text-align: center; line-height: 32px; cursor: pointer; margin-left: 5px;}
.btn-search{ padding: 0 15px; float: right; margin-right: 55px;}
.btn-group{ width: 100%; text-align: center; margin: 0 auto;}
.btn-group-left{ float: left; padding-left: 6px;}
/*搜索*/
.search{ width: 100%; height: auto; padding: 10px 0 16px 0; position:relative; z-index: 10; background: #fafafa; border-bottom: 1px solid #dbdbdc;/* border-right: 0; border-top: 0;*/ box-sizing: border-box; }
.search dl{ width: auto; height: 26px; float: left; padding-top: 6px;}
.search dl dt{ width: 125px; height: 26px;line-height: 26px; padding-right: 7px; color: #575757; float: left; text-align: right;}
.search dl dt span{ color: #f40;}
.search dl dd input{ width: 178px; height: 24px; line-height: 24px; padding: 0 10px; border: 1px solid #c7c7c7; float: left; background: #fff; color: #7e7e7e;}
.search dl dd input.ui-inputtext{ width: 100px; background:#fff url(assets/images/datePickerDefault.png) no-repeat 94px 1px;}
.search dl dd input.ui-inputtext:hover{background:#fff url(assets/images/datePicker.png) no-repeat 94px 1px;}
/*.search dl dd .tl-drop-down{ width: 188px;}*/
/*搜索文字高亮*/
.highlight{ color: #2da3b1}
