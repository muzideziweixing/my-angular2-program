<ng-container>
  <tl-opr-img-details-query (onQuery)="query($event)"></tl-opr-img-details-query>
  <tl-opr-img-details-total [goodsInfo]="goodsInfo" [totalInfo]="totalInfo"></tl-opr-img-details-total>
  <tl-opr-img-details-table [dataTableValue]="dataTableValue"  [pagingBoxObj]="pagingBoxObj"  (onPageChange)="pageChange($event)"></tl-opr-img-details-table>
</ng-container>