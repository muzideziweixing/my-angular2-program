<tl-data-table [dataTableObj]="dataTableObj" [dataTableValue]="dataTableValue" [pagingBoxObj]="pagingBoxObj" (onPageChange)="pageChange($event)">
  <template tl-template="date" let-rowData>
    <p>{{rowData.date}}</p>
  </template>
  <template tl-template="trade_number" let-rowData>
    <p>{{rowData.trade_number}}笔</p>
  </template>
  <template tl-template="trade_total" let-rowData>
    <p>￥{{rowData.trade_total}}</p>
  </template>
  <template tl-template="trade_income" let-rowData>
    <p>￥{{rowData.trade_income}}</p>
  </template>
  <template tl-template="trade_charge" let-rowData>
    <p>￥{{rowData.trade_charge}}</p>
  </template>
  <template tl-template="oper" let-rowData>
    <a class="btn-details" (click)="clickDetails(rowData)"></a> 
  </template>
</tl-data-table>