<div class="search clearfix">
  <dl>
    <dt><label>账单状态&nbsp;:&nbsp;&nbsp;</label></dt>
    <dd>
      <tl-drop-down-box [dropDownBoxObj]="dropDownBoxObj" [dropDownBoxValue]="settlementValue.value" [keyName]="'name'" [keyId]="'id'" [(ngModel)]="settlement" 
      [placeholder]="'未申请结算'" > </tl-drop-down-box>
      <!--<tl-drop-down-box [dropDownBoxObj]="dropDownBoxObj" [(ngModel)]="status" [placeholder]="'未申请结算'"></tl-drop-down-box>-->
      </dd>
  </dl>
  <dl>
    <dt><span class="red">*&nbsp;</span><label>对账日期&nbsp;:&nbsp;&nbsp;</label></dt>
    <dd>
        <tl-start-end-calendar [(ngModel)]="beginEndDate"></tl-start-end-calendar>  
    </dd>
  </dl> 
  <button class="btn btn-blue btn-search" href="javascript:void(0);" (click)="query()">搜索</button> 
</div>
