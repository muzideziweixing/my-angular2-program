<tl-data-table [dataTableObj]="dataTableObj" [dataTableValue]="dataTableValue" [pagingBoxObj]="pagingBoxObj" (onPageChange)="pageChange($event)">
  <template tl-template="serial_number" let-rowData>
    <p>{{rowData._index+1}}</p>
  </template>
  <template tl-template="definite_message" let-rowData>
    <strong (click)="checkNews(rowData)" [ngClass]="{'checked': rowData.status=='1'}">{{rowData.news}}</strong>
    <span>{{rowData.time}}</span>
  </template>
  <template tl-template="oper" let-rowData>
    <a class="btn-delete" (click)="deleteNews(rowData)"></a>
  </template>
</tl-data-table>